{
  "name": "@elastic.io/component-commons-library",
  "version": "2.0.1-dev.10",
  "description": "Library for most common component development cases",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "author": {
    "name": "elastic.io GmbH",
    "email": "info@elastic.io",
    "url": "http://elastic.io"
  },
  "keywords": [
    "elasticio",
    "ipaas",
    "elasticio-component"
  ],
  "scripts": {
    "tsc": "rm -fr dist && tsc",
    "lint": "eslint '*/**/*.ts' --quiet --fix",
    "test": "NODE_ENV=test nyc mocha -r ts-node/register --recursive ./spec/**/*.ts",
    "pretest": "npm run lint && find src spec -name \"*.js\" -type f -delete",
    "posttest": "npm run tsc",
    "build": "npm run tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@elastic.io/jsonata-moment": "1.1.4",
    "@elastic.io/maester-client": "3.4.2",
    "@elastic.io/ntlm-client": "1.0.0",
    "async": "3.2.0",
    "axios": "0.24.0",
    "bunyan": "1.8.14",
    "bunyan-format": "0.2.1",
    "bunyan-serializers": "0.0.2",
    "elasticio-node": "0.0.9",
    "remove-leading-slash": "1.0.1",
    "remove-trailing-slash": "0.1.1",
    "request": "2.88.2"
  },
  "devDependencies": {
    "@types/async": "^3.2.12",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.4",
    "@types/mocha": "9.0.0",
    "@types/node": "16.9.6",
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.24.2",
    "mocha": "9.2.2",
    "nock": "13.0.4",
    "nyc": "15.1.0",
    "sinon": "11.1.2",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "4.4.3"
  },
  "repository": "elasticio/component-commons-library",
  "license": "Apache-2.0"
}