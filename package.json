{
  "name": "@elastic.io/component-commons-library",
  "version": "2.0.0-dev.2",
  "description": "Library for most common component development cases",
  "author": {
    "name": "elastic.io GmbH",
    "email": "info@elastic.io",
    "url": "http://elastic.io"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "elasticio",
    "ipaas",
    "elasticio-component"
  ],
  "scripts": {
    "tsc": "rm -fr out && tsc",
    "pretest": "tslint -c tslint.json --project tsconfig.json  lib/**/*.ts spec/**/*.ts --fix",
    "preparepublish": "npm run tsc",
    "prepublishOnly": "tsc",
    "test": "LOG_LEVEL=trace LOG_OUTPUT_MODE=long mocha --require ts-node/register test/**/*.ts --timeout 50000",
    "posttest": "npm run tsc",
    "integration-test": "npm run pretest && mocha --require ts-node/register \"spec-integration/**/*spec.ts\" --timeout 10000"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@elastic.io/jsonata-moment": "1.1.4",
    "@elastic.io/maester-client": "3.4.1",
    "@elastic.io/ntlm-client": "1.0.0",
    "@types/node": "14.14.3",
    "async": "3.2.0",
    "axios": "0.24.0",
    "bunyan": "1.8.14",
    "bunyan-format": "0.2.1",
    "bunyan-serializers": "0.0.2",
    "elasticio-node": "0.0.9",
    "remove-leading-slash": "1.0.1",
    "remove-trailing-slash": "0.1.1",
    "request": "2.88.2",
    "typescript": "4.0.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "dotenv": "10.0.0",
    "mocha": "8.4.0",
    "nock": "13.0.4",
    "sinon": "9.2.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "tslint-config-airbnb": "5.11.2"
  },
  "repository": "elasticio/component-commons-library",
  "license": "Apache-2.0"
}